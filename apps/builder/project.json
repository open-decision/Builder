{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/builder",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "root": "apps/builder",
        "outputPath": "dist/apps/builder"
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "options": {
        "buildTarget": "builder:build",
        "dev": true,
        "hostname": "localhost"
      },
      "configurations": {
        "production": {
          "dev": false,
          "buildTarget": "builder:build",
          "hostname": "localhost"
        },
        "development": {
          "buildTarget": "builder:build",
          "dev": true,
          "hostname": "localhost"
        }
      }
    },
    "serve:app": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx run-many --target=serve --projects=builder,api --parallel=2 --output-style=stream"
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": "dependencies"
        }
      ]
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "builder:build"
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "configurations": {
        "watch": {
          "watch": true,
          "passWithNoTests": true
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/builder/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vercel deploy --force"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p tsconfig.json",
        "cwd": "apps/builder"
      },
      "dependsOn": [
        {
          "projects": "dependencies",
          "target": "build"
        }
      ]
    }
  },
  "tags": [],
  "implicitDependencies": ["api"],
  "name": "builder"
}
