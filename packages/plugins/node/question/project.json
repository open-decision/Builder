{
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/plugins/node/question/node/src",
  "projectType": "library",
  "tags": ["scope:question-node-plugin"],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/plugins/node/question/node/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/packages/plugins/node/question"],
      "options": {
        "jestConfig": "packages/plugins/node/question/node/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p tsconfig.lib.json",
        "cwd": "packages/plugins/node/question/node"
      }
    }
  }
}
