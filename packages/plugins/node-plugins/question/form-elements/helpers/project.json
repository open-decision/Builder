{
  "$schema": "../../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/plugins/node-plugins/question/form-elements/helpers/src",
  "projectType": "library",
  "tags": ["scope:question-node-plugin"],
  "targets": {
    "build": {
      "executor": "@nrwl/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/plugins/node-plugins/question/form-elements/helpers",
        "main": "packages/plugins/node-plugins/question/form-elements/helpers/src/index.ts",
        "tsConfig": "packages/plugins/node-plugins/question/form-elements/helpers/tsconfig.lib.json",
        "assets": [
          "packages/plugins/node-plugins/question/form-elements/helpers/*.md"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/plugins/node-plugins/question/form-elements/helpers/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "typecheck": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "tsc -p tsconfig.lib.json",
        "cwd": "packages/plugins/node-plugins/question/form-elements/helpers"
      }
    }
  }
}
