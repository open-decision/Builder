{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/node-plugins/question/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/node-plugins/question",
        "main": "packages/node-plugins/question/src/index.ts",
        "tsConfig": "packages/node-plugins/question/tsconfig.lib.json",
        "assets": ["packages/node-plugins/question/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/node-plugins/question/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/node-plugins/question"],
      "options": {
        "jestConfig": "packages/node-plugins/question/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "typecheck": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "tsc -p tsconfig.lib.json",
        "cwd": "packages/node-plugins/question"
      }
    }
  }
}
