{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/prisma/src",
  "projectType": "library",
  "targets": {
    "serve:studio": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "pnpm prisma studio",
        "cwd": "packages/prisma"
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "prisma generate --schema=./src/schema.prisma",
        "cwd": "packages/prisma"
      }
    },
    "migrate:deploy": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "prisma migrate deploy --schema=./src/schema.prisma",
        "cwd": "packages/prisma"
      }
    }
  },
  "tags": []
}
