{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/prisma/src",
  "projectType": "library",
  "targets": {
    "serve:studio": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm prisma studio",
        "cwd": "packages/prisma"
      }
    },
    "generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm prisma generate --schema=./src/schema.prisma",
        "cwd": "packages/prisma"
      }
    },
    "build:lib": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/prisma",
        "main": "packages/prisma/src/index.ts",
        "tsConfig": "packages/prisma/tsconfig.lib.json",
        "assets": ["packages/prisma/*.md", "node_modules/.prisma"]
      },
      "dependsOn": [
        {
          "target": "generate",
          "projects": "self"
        }
      ]
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cp -r node_modules/.prisma ../../dist/packages/prisma/node_modules",
        "cwd": "packages/prisma"
      },
      "dependsOn": [
        {
          "target": "build:lib",
          "projects": "self"
        }
      ]
    },
    "migrate:deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma migrate deploy --schema=./src/schema.prisma",
        "cwd": "packages/prisma"
      }
    },
    "migrate:dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma migrate dev --schema=./src/schema.prisma",
        "cwd": "packages/prisma"
      }
    }
  },
  "tags": []
}
